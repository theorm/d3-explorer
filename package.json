  "jsdelivr": "dist/d3-explorer.min.js",
  "module": "src/index.js",
  "devDependencies": {
    "eslint": "6",
    "rollup": "1",
    "rollup-plugin-node-resolve": "5",
    "rollup-plugin-terser": "^5.1.1"
  },
  "dependencies": {
